<refentry id="mauibuild-make">
  <refentryinfo>
    <title>mauibuild Manual</title>
    <productname>mauibuild</productname>
    <date>May 2013</date>
    <authorgroup>
      <author>
        <contrib>Developer</contrib>
        <firstname>Pier Luigi</firstname>
        <surname>Fiorini</surname>
        <email>pierluigi.fiorini@maui-project.org</email>
      </author>
    </authorgroup>
  </refentryinfo>

  <refmeta>
    <refentrytitle>mauibuild-make</refentrytitle>
    <manvolnum>1</manvolnum>
  </refmeta>

  <refnamediv>
    <refname>mauibuild-make</refname>
    <refpurpose>Execute tasks</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <cmdsynopsis>
      <command>mauibuild make</command>
      <arg choice="opt"><option>options</option></arg>
      <arg choice="opt">--<arg choice="req" rep="repeat"><option>arguments</option></arg></arg>
    </cmdsynopsis>
  </refsynopsisdiv>

  <refsect1>
    <title>Description</title>
    <para>
      Execute tasks.
    </para>
  </refsect1>

  <refsect1>
    <title>Options</title>
    <variablelist>
      <varlistentry>
        <term>
          <option>-l, --list</option>
        </term>
        <listitem>
          <para>List all available tasks and exit.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>-n, --task-name</option>
        </term>
        <listitem>
          <para>Specify name of the task to execute.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>-u, --only</option>
        </term>
        <listitem>
          <para>Don't process tasks after this.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>--task-help</option>
        </term>
        <listitem>
          <para>Show help message for the specified task and exit.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>-x, --skip NAME</option>
        </term>
        <listitem>
          <para>Skip the task <varname>NAME</varname>.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>-- arguments...</option>
        </term>
        <listitem>
          <para>Arguments for the specified task.</para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsect1>

  <refsect1>
    <title>Tasks</title>
    <variablelist>
      <varlistentry>
        <term>
          <option>resolve</option>
        </term>
        <listitem>
          <para>Expand git revisions in source to exact targets.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>build</option>
        </term>
        <listitem>
          <para>Build multiple components and generate trees.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>bdiff</option>
        </term>
        <listitem>
          <para>Report differences between builds.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>builddisks</option>
        </term>
        <listitem>
          <para>Generate uncompressed disk images updated with ostree.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>zdisks</option>
        </term>
        <listitem>
          <para>Generate compressed disk images.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>buildlive</option>
        </term>
        <listitem>
          <para>Generate live images.</para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsect1>

  <refsect1>
    <title>See Also</title>
    <para>
      <citerefentry><refentrytitle>mauibuild</refentrytitle><manvolnum>1</manvolnum></citerefentry>
    </para>
  </refsect1>
</refentry>
