<!DOCTYPE refentry PUBLIC "-//OASIS//DTD DocBook XML V4.2//EN"
          "http://www.oasis-open.org/docbook/xml/4.2/docbookx.dtd">
<refentry id="mauibuild-checkout">
  <refentryinfo>
    <title>mauibuild Manual</title>
    <productname>mauibuild</productname>
    <date>May 2013</date>
    <authorgroup>
      <author>
        <contrib>Developer</contrib>
        <firstname>Pier Luigi</firstname>
        <surname>Fiorini</surname>
        <email>pierluigi.fiorini@maui-project.org</email>
      </author>
    </authorgroup>
  </refentryinfo>

  <refmeta>
    <refentrytitle>mauibuild-checkout</refentrytitle>
    <manvolnum>1</manvolnum>
  </refmeta>

  <refnamediv>
    <refname>mauibuild-checkout</refname>
    <refpurpose>Check out one or more git repositories</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <cmdsynopsis>
      <command>mauibuild checkout</command>
      <arg choice="opt"><option>options</option></arg>
      <arg choice="req" rep="repeat"><option>components</option></arg>
    </cmdsynopsis>
  </refsynopsisdiv>

  <refsect1>
    <title>Description</title>
    <para>
      Checks out one or more git repositories.
    </para>
  </refsect1>

  <refsect1>
    <title>Options</title>
    <variablelist>
      <varlistentry>
        <term>
          <option>--overwrite</option>
        </term>
        <listitem>
          <para>Overwrite.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>--patches-path PATH</option>
        </term>
        <listitem>
          <para>Specify the path where patches are saved.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>--metadata-path PATH</option>
        </term>
        <listitem>
          <para>Specify the path where metadata is located.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>--workdir PATH</option>
        </term>
        <listitem>
          <para>Specify an alternative working directory.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>--snapshot PATH</option>
        </term>
        <listitem>
          <para>Specify an alternative snapshot file.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>--checkoutdir PATH</option>
        </term>
        <listitem>
          <para>Specify where the git repositories will be checked out.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>--clean</option>
        </term>
        <listitem>
          <para>Clean.</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>
          <option>components...</option>
        </term>
        <listitem>
          <para>One or more components whose git repositories will be checked out.</para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsect1>

  <refsect1>
    <title>See Also</title>
    <para>
      <citerefentry><refentrytitle>mauibuild</refentrytitle><manvolnum>1</manvolnum></citerefentry>
    </para>
  </refsect1>
</refentry>
